{
  "version": "0.1.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "src/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --entry ./src/index.js",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build --entry ./src/index.js",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "effector-when.macro",
  "author": "Anton Kosykh",
  "module": "dist/effector-when.macro.esm.js",
  "size-limit": [
    {
      "path": "dist/effector-when.macro.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/effector-when.macro.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "babel-plugin-macros": "^3.1.0",
    "invariant": "^2.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/traverse": "^7.15.4",
    "@types/babel-plugin-macros": "^2.8.5",
    "@types/invariant": "^2.2.35",
    "tsdx": "^0.14.1"
  },
  "peerDependencies": {
    "effector": "^22.1.2"
  }
}
